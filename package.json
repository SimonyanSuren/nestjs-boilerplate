{
  "name": "boilerplate-api",
  "version": "0.0.1",
  "description": "Boilerplate API",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=dev nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch --preserveWatchOutput",
    "start:debug": "cross-env NODE_ENV=dev nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,test}/**/*.ts' --cache",
    "lint:fix": "eslint '{src,test}/**/*.ts' --fix",
    "lint:view": "npx eslint-flat-config-viewer",
    "hygen": "hygen new module",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch --maxWorkers=1",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --maxWorkers=1",
    "typeorm": "NODE_ENV=dev ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./dist/common/db/orm-config.js",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/common/db/migrations/$npm_config_name",
    "migration:generate": "npm run typeorm migration:generate ./src/common/db/migrations/$npm_config_name",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:show": "npm run typeorm migration:show",
    "schema:drop": "npm run typeorm schema:drop",
    "docker:build": "docker compose --env-file .env.dev -f docker-compose.dev.yml build --no-cache",
    "docker:up": "docker compose --env-file .env.dev -f docker-compose.dev.yml up -d",
    "docker:build-up": "docker compose --env-file .env.dev -f docker-compose.dev.yml up -d --build",
    "docker:build-up:api": "docker compose --env-file .env.dev -f docker-compose.dev.yml up -d --build --no-deps boilerplate-api",
    "docker:down": "docker compose --env-file .env.dev -f docker-compose.dev.yml down -v",
    "docker:build:prod": "docker compose --env-file .env.dev -f docker-compose.prod.yml build --no-cache",
    "docker:up:api:prod": "docker compose --env-file .env.dev -f docker-compose.prod.yml up -d --no-deps boilerplate-api",
    "docker:prod": "npm run docker:build:prod && npm run docker:up:api:prod"
  },
  "dependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/common": "10.3.3",
    "@nestjs/config": "3.2.0",
    "@nestjs/core": "10.3.3",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.8",
    "@nestjs/swagger": "7.3.0",
    "@nestjs/typeorm": "10.0.2",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "date-fns": "3.6.0",
    "dotenv": "16.4.5",
    "express-basic-auth": "1.2.1",
    "helmet": "7.1.0",
    "libphonenumber-js": "1.10.58",
    "lodash": "4.17.21",
    "nestjs-pino": "4.0.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "8.11.3",
    "pino": "8.19.0",
    "pino-http": "9.0.0",
    "pino-pretty": "11.0.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "5.0.5",
    "rxjs": "^7.8.1",
    "swagger-stats": "0.99.7",
    "swagger-ui-express": "5.0.0",
    "typeorm": "0.3.20",
    "typeorm-transactional": "0.5.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.0",
    "@commitlint/config-conventional": "19.1.0",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.3",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/bcrypt": "5.0.2",
    "@types/body-parser": "1.19.5",
    "@types/compression": "1.7.5",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/lodash": "4.14.202",
    "@types/node": "20.11.24",
    "@types/passport": "1.0.16",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/prompts": "2.4.9",
    "@types/supertest": "6.0.2",
    "@types/swagger-stats": "0.95.11",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-canonical": "4.18.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-jsonc": "2.13.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sonarjs": "0.24.0",
    "eslint-plugin-unicorn": "51.0.1",
    "husky": "9.0.11",
    "hygen": "6.2.11",
    "jest": "29.7.0",
    "jsonc-eslint-parser": "2.4.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "run-script-webpack-plugin": "0.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.2",
    "webpack": "5.90.3",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
